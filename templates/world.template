# set the resolution of the underlying raytrace model in meters
resolution <rt_resolution>

interval_sim <sim_interval>  # simulation timestep in milliseconds

# load an environment bitmap
model
(
  name  "map"
  color "gray30"

  boundary 1

  gui_nose    0
  gui_grid    0
  gui_outline 0

  gripper_return  0
  fiducial_return 0
  ranger_return   1

  bitmap "<map_filename>"
  size   [ <map_width> <map_height> 0.5 ]
  origin [ <map_x> <map_y> 0 0 ]
)
